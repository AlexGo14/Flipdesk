h1 Agents
button(class="btn btn-primary") Add agent
<br>
table(class="table table-striped table-hover table-condensed")
	tr
		td ID
		td Last name
		td First name
		td Email
		td Active
	tr(onclick="toggle_edit_agent_modal(1)")
		td 1
		td Giere
		td Johannes
		td jogiere@gmail.com
		td
			label
				input(type="checkbox" checked)
	tr(onclick="toggle_edit_agent_modal(2)")
		td 2
		td Nagel
		td Karl-Heinz
		td jogiere@gmail.com
		td
			label
				input(type="checkbox")
	
div(class="modal fade" id="edit_agent_modal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true")
		div(class="modal-dialog")
			div(class="modal-content")
				div(class="modal-header")
					button(type="button" class="close" data-dismiss="modal")
						span(aria-hidden="true") &times;
						span(class="sr-only") Close
					h4(class="modal-title" id="myModalLabel") Edit agent
				div(class="modal-body")
					form(class="form" role="form" id="edit_agent_form")
						div(class="form-group")
							input(type="text" class="form-control input-lg" placeholder="Caption" id="create_ticket_form_caption")
							<br><br>
							label Agent: 
							<br><br>
				div(class="modal-footer")
					button(type="button" class="btn btn-default btn-lg" data-dismiss="modal") Close
					button(type="button" class="btn btn-primary btn-lg" onclick="ticket_send()") Send
			
